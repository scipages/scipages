{
  "name": "scipages",
  "version": "0.0.1",
  "description": "A CMS and Static Site Generator for academic websites",
  "productName": "SciPages",
  "author": "Christos",
  "private": true,
  "scripts": {
    "dev": "npm run rm-gen && npm run link-gen && quasar dev -m electron",
    "build": "npm run rm-gen && npm run install-gen-pack && quasar build -m electron",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "icons": "icongenie generate -i AppIcon.png --skip-trim --theme-color 1976d2",
    "link-gen": "npm link ../gen --save",
    "install-gen-pack": "cp ../gen/scipages-gen-0.0.1.tgz ./scipages-gen-package.tgz && npm install ./scipages-gen-package.tgz --save",
    "rm-gen": "npm rm @scipages/gen"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.5",
    "@scipages/gen": "file:../gen",
    "archiver": "^5.3.0",
    "axios": "^0.27.2",
    "core-js": "^3.13.1",
    "electron-store": "^8.0.0",
    "extract-zip": "^2.0.1",
    "lodash": "^4.17.20",
    "lowdb": "^1.0.0",
    "quasar": "^2.12.3",
    "semver": "^7.3.5",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.1.6",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@quasar/app-webpack": "^3.9.2",
    "@types/archiver": "^5.1.0",
    "@types/lowdb": "^1.0.10",
    "@types/node": "^17.0.18",
    "@types/semver": "^7.3.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vue/devtools": "^6.0.0-beta.15",
    "babel-eslint": "^10.0.1",
    "devtron": "^1.4.0",
    "electron": "^22.0.0",
    "electron-builder": "^22.10.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^15.4.0",
    "eslint": "^8.9.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
